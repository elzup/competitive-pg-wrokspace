{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": false,
    "noEmitOnError": true,
    "rootDir": ".",
    "strictPropertyInitialization": false
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}